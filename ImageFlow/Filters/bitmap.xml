<?xml version="1.0"?>
<filter>
  <name>bitmap</name>
  <description>Draw bitmap image with various tools.</description>
  <expression>
    <operation name="blend">
      <operation name="constant-color">
        <variable name="defaultColor"/>
      </operation>
      <variable name="drawing"/>
      <constant type="integer">0</constant>
    </operation>
  </expression>
  <settings_nib>IFBitmapSource</settings_nib>
  <delegate_class>IFBitmapDrawingSourceDelegate</delegate_class>
</filter>
