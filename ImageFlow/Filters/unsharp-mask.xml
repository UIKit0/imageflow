<?xml version="1.0"?>
<filter>
  <name>unsharp-mask</name>
  <description>Increases the contrast of the edges between pixels of different colors in an image.</description>
  <expression>
    <operation name="unsharp-mask">
      <parent index="0"/>
      <variable name="intensity"/>
      <variable name="radius"/>
    </operation>
  </expression>
  <min_parents>1</min_parents>
  <max_parents>1</max_parents>
  <settings_nib>IFUnsharpMaskFilter</settings_nib>
  <delegate_class>IFUnsharpMaskFilterDelegate</delegate_class>
</filter>
