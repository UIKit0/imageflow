<?xml version="1.0"?>
<filter>
  <name>crop</name>
  <description>Crop input image</description>
  <expression>
    <operation name="crop">
      <parent index="0"/>
      <variable name="rectangle"/>
    </operation>
  </expression>
  <min_parents>1</min_parents>
  <max_parents>1</max_parents>
  <settings_nib>IFCropFilter</settings_nib>
  <delegate_class>IFCropFilterDelegate</delegate_class>
</filter>
