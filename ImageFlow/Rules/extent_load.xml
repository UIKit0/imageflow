<?xml version="1.0"?>
<rule>
  <operation name="extent">
    <operation name="load">
      <variable name="fileName"/>
      <variable name="useEmbeddedProfile"/>
      <variable name="defaultRGBProfileFileName"/>
      <variable name="defaultGrayProfileFileName"/>
      <variable name="defaultCMYKProfileFileName"/>
      <variable name="useEmbeddedResolution"/>
      <variable name="useDocumentResolutionAsDefault"/>
      <variable name="defaultResolutionX"/>
      <variable name="defaultResolutionY"/>
    </operation>
  </operation>
  <operation name="file-extent">
    <wildcard name="fileName"/>
  </operation>
</rule>
