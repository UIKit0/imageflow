<?xml version="1.0"?>
<tree-template>
  <name>Draw bitmap</name>
  <description>Draw bitmap image</description>
  <tree>
    <filter id="1">
      <name>IFBitmapDrawingSource</name>
      <settings>
        <key>drawing</key>
        <expression>&lt;primitive name="empty"/&gt;</expression>
        <key>defaultColor</key>
        <color>0 0 0 0</color>
        <key>brushShape</key>
        <number>0</number>
        <key>brushSize</key>
        <number>10</number>
        <key>brushColor</key>
        <color>0 0 0 1</color>
        <key>brushMode</key>
        <integer>0</integer>
        <key>brushOpacity</key>
        <number>1</number>
      </settings>
    </filter>
  </tree>  
</tree-template>
