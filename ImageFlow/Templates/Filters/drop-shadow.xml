<?xml version="1.0"?>
<imageflow>
  <title>Drop shadow</title>
  <description>Drop shadow</description>
  <tree id="1">
    <filter>
      <name>IFDropShadowFilter</name>
      <env>
        <key>color</key>
        <color>
          0 0 0 1.0
        </color>
        <key>offset</key>
        <point>{10,10}</point>
        <key>blur</key>
        <number>3</number>
      </env>
    </filter>
    <tree id="2">
      <filter>
        <name>IFGhostFilter</name>
      </filter>
    </tree>
  </tree>
</imageflow>
