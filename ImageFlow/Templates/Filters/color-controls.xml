<?xml version="1.0"?>
<imageflow>
  <title>Color controls</title>
  <description>Control contrast, brightness and saturation</description>
  <tree id="1">
    <filter>
      <name>color-controls</name>
      <env>
        <key>contrast</key>
        <number>1</number>
        <key>brightness</key>
        <number>0</number>
        <key>saturation</key>
        <number>1</number>
      </env>
    </filter>
    <tree id="2">
      <filter>
        <name>nop</name>
      </filter>
    </tree>
  </tree>
</imageflow>