<?xml version="1.0"?>
<imageflow>
  <title>Crop</title>
  <description>Crop input image</description>
  <tree id="1">
    <filter>
      <name>IFCropFilter</name>
      <env>
        <key>rectangle</key>
        <rect>{{0,0}{100,100}}</rect>
      </env>
    </filter>
    <tree id="2">
      <filter>
        <name>IFGhostFilter</name>
      </filter>
    </tree>
  </tree>
</imageflow>
