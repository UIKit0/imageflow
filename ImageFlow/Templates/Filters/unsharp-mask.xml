<?xml version="1.0"?>
<imageflow>
  <title>Unsharp mask</title>
  <description>Increases the contrast of the edges between pixels of different colors in an image.</description>
  <tree id="1">
    <filter>
      <name>IFUnsharpMaskFilter</name>
      <env>
      	<key>intensity</key>
        <number>0.5</number>
        <key>radius</key>
        <number>2.5</number>
      </env>
    </filter>
    <tree id="2">
      <filter>
        <name>IFGhostFilter</name>
      </filter>
    </tree>
  </tree>
</imageflow>
