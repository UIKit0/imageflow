<?xml version="1.0"?>
<tree-template>
  <name>Unsharp mask</name>
  <description>Increases the sharpness of input image.</description>
  <tree>
    <filter id="1">
      <name>IFUnsharpMaskFilter</name>
      <settings>
      	<key>intensity</key>
        <number>0.5</number>
        <key>radius</key>
        <number>2.5</number>
      </settings>
      <parents>
        <hole id="2"/>
      </parents>
    </filter>
  </tree>
</tree-template>
