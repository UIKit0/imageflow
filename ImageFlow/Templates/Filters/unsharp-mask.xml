<?xml version="1.0"?>
<tree-template>
  <name>Unsharp mask</name>
  <description>Increases the sharpness of input image.</description>
  <tree>
    <node id="1">
      <filter>IFUnsharpMaskFilter</filter>
      <settings>
      	<key>intensity</key>
        <number>0.5</number>
        <key>radius</key>
        <number>2.5</number>
      </settings>
    </node>
  </tree>
</tree-template>
