<?xml version="1.0"?>
<imageflow>
  <title>Threshold</title>
  <description>Threshold input image</description>
  <tree id="1">
    <filter>
      <name>IFThresholdFilter</name>
      <env>
        <key>threshold</key>
        <number>0.5</number>
      </env>
    </filter>
    <tree id="2">
      <filter>
        <name>IFGhostFilter</name>
      </filter>
    </tree>
  </tree>
</imageflow>
